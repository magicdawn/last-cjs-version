{
  "name": "last-cjs-version",
  "version": "0.0.2",
  "description": "get last cjs version of a module ",
  "main": "index.js",
  "bin": {
    "last-cjs-version": "./bin/last-cjs-version"
  },
  "files": [
    "lib",
    "bin/last-cjs-version",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "test-cover": "jest --coverage",
    "report-cover": "codecov",
    "build": "rm -rf lib; rm tsconfig.tsbuildinfo; tsc",
    "prepublishOnly": "npm run build",
    "gen-readme": "swig render ./_readme/readme.md > README.md && prettier --write README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/last-cjs-version.git"
  },
  "keywords": [
    "cjs",
    "last-version",
    "esm",
    "sindre",
    "sindresorhus"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/last-cjs-version/issues"
  },
  "homepage": "https://github.com/magicdawn/last-cjs-version#readme",
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.2",
    "@swc/core": "^1.3.14",
    "@swc/helpers": "^0.4.12",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "@types/should": "^13.0.0",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "prettier": "^2.7.1",
    "should": "^13.2.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "execa": "5",
    "got": "11",
    "lodash": "^4.17.21",
    "proxy-agent": "^5.0.0",
    "superagent": "^8.0.3",
    "type-fest": "^3.1.0",
    "yargs": "^17.6.2"
  }
}
