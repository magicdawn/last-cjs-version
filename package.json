{
  "name": "last-cjs-version",
  "version": "0.1.2",
  "description": "get last cjs version of a module ",
  "main": "index.js",
  "bin": {
    "last-cjs-version": "./bin/last-cjs-version"
  },
  "files": [
    "lib",
    "bin/last-cjs-version",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "mocha",
    "test-cover": "nyc --reporter lcov --reporter text mocha",
    "build": "rm -rf lib; rm tsconfig.tsbuildinfo; tsc",
    "prepublishOnly": "npm run build",
    "gen-readme": "swig render ./_readme/readme.md > README.md && prettier --write README.md"
  },
  "dependencies": {
    "chalk": "4",
    "execa": "5",
    "got": "11",
    "lodash": "^4.17.21",
    "preferred-pm": "^3.0.3",
    "proxy-agent": "^5.0.0",
    "type-fest": "^3.3.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.2",
    "@swc/core": "^1.3.21",
    "@swc/helpers": "^0.4.14",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.10",
    "@types/yargs": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.0",
    "should": "^13.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/last-cjs-version.git"
  },
  "keywords": [
    "cjs",
    "last-version",
    "esm",
    "sindre",
    "sindresorhus"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/last-cjs-version/issues"
  },
  "homepage": "https://github.com/magicdawn/last-cjs-version#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@7.17.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
